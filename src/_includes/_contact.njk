<form action="https://formspree.io/f/movjaodg" id="contact-form" method="POST">
  <stack-l>
    <label for="name">Name<span>*</span></label>
    <input
      id="name"
      type="text"
      name="name"
      maxLength="256"
      autocorrect="off"
      spellcheck="false"
      required
    />
    <label for="email">Email<span>*</span></label>
    <input
      id="email"
      type="email"
      name="_replyto"
      maxLength="256"
      autocapitalize="none"
      autocorrect="off"
      spellcheck="false"
      required
    />
    <label for="subject">Subject<span>*</span></label>
    <input
      id="subject"
      type="text"
      name="_subject"
      maxLength="256"
      required
    />
    <div class="sr-only">
      <label for="vorkenblx">Skip this field if youâ€™re human<span>*</span></label>
      <input
        id="vorkenblx"
        type="text"
        name="_gotcha"
        maxLength="64"
      />
    </div>
    <label for="message">Message<span>*</span></label>
    <textarea
      id="message"
      name="message"
      maxLength="4096"
      rows="10"
      required
    ></textarea>
    <button
      class="g-recaptcha button button-primary" data-sitekey="6LeX8L8qAAAAAI6RxHWpI99xi_H1WB8DM3gEU9GX"
      data-callback="onSubmit"
      data-action="submit">
        Send it!
    </button>
  </stack-l>
</form>
